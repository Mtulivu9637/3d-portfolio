{
  "name": "3d-portfolio-developer",
  "version": "1.0.0",
  "description": "A cutting-edge 3D portfolio website for iOS Swift, Python/Django, and React developers",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "python -m http.server 8000",
    "test": "echo \"No tests yet\" && exit 0",
    "lint": "eslint src --ext .js",
    "format": "prettier --write src/**/*.js",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "3d",
    "portfolio",
    "three.js",
    "anime.js",
    "webgl",
    "particles",
    "glitch",
    "neon",
    "ios",
    "swift",
    "python",
    "django",
    "react",
    "developer"
  ],
  "author": "Software Developer",
  "license": "MIT",
  "dependencies": {
    "three": "^0.157.0",
    "animejs": "^3.2.1"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "gh-pages": "^6.1.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/3d-portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/3d-portfolio/issues"
  },
  "homepage": "https://yourusername.github.io/3d-portfolio"
}
